{
  "version": "1.0",
  "defaultProvider": "unpkg",
  "libraries": [
    {
      "library": "@aspnet/signalr@1.0.3",
      "destination": "wwwroot/aspnet/signalr/",
      "files": [
        "dist/browser/signalr.min.js"
      ]
    },
    {
      "provider": "cdnjs",
      "library": "bootstrap@4.6.0",
      "destination": "wwwroot/bootstrap/"
    },
    {
      "provider": "cdnjs",
      "library": "datatables.net@2.1.1",
      "destination": "wwwroot/datatables.net/",
      "files": [
        "jquery.dataTables.min.js"
      ]
    },
    {
      "provider": "cdnjs",
      "library": "datatables.net-bs4@3.2.2",
      "destination": "wwwroot/datatables.net-bs4/"
    }
  ]
}